{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "command": "npm run build"
      }
    ],
    "routes": [
      {
        "src": "/api/*",
        "dest": "server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }